{
  "family": "daisy-panel-task",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512",
  "executionRoleArn": "arn:aws:iam::529095162677:role/ecsTaskExecutionRole",
  "taskRoleArn": "arn:aws:iam::529095162677:role/ecsTaskRole",
  "containerDefinitions": [
    {
      "name": "daisy-panel",
      "image": "529095162677.dkr.ecr.us-east-1.amazonaws.com/daisy-panel:latest",
      "essential": true,
      "portMappings": [
        {
          "containerPort": 3000,
          "protocol": "tcp"
        }
      ],
      "environment": [
        {
          "name": "RAILS_ENV",
          "value": "production"
        },
        {
          "name": "PORT",
          "value": "3000"
        },
        {
          "name": "WEB_CONCURRENCY",
          "value": "1"
        },
        {
          "name": "SOLID_QUEUE_IN_PUMA",
          "value": "true"
        }
      ],
      "secrets": [
        {
          "name": "RAILS_MASTER_KEY",
          "valueFrom": "arn:aws:secretsmanager:us-east-1:529095162677:secret:daisy-panel/rails-master-key-2wM2Mx"
        },
        {
          "name": "DATABASE_URL",
          "valueFrom": "arn:aws:secretsmanager:us-east-1:529095162677:secret:daisy-panel/database-url-2mWue8"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/daisy-panel",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "ecs"
        }
      },
      "healthCheck": {
        "command": ["CMD-SHELL", "curl -f http://localhost:3000/ || exit 1"],
        "interval": 30,
        "timeout": 5,
        "retries": 3,
        "startPeriod": 60
      }
    }
  ]
}